{
  "extends": "../../tsconfg.json",
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "outDir": "dist",
    "jsx": "react-jsx",
    "noEmit": true,
    "isolatedModules": true,
    "skipLibCheck": true,
    "allowJs": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext"
  },
  "include": [
    "src"
  ]
}
